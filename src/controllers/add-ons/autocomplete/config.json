{
    "resourceTypes": {
        "identity.Workspace": {
            "request": {
                "search": ["workspace_id", "name"],
                "only": ["workspace_id", "name"]
            },
            "response": {
                "key": "workspace_id",
                "name": "<%- name %>"
            }
        },
        "identity.Project": {
            "request": {
                "search": ["project_id", "name"],
                "only": ["project_id", "name", "project_group_id"]
            },
            "response": {
                "key": "project_id",
                "name": "<%- name %>"
            }
        },
        "identity.ProjectGroup": {
            "request": {
                "search": ["project_group_id", "name"]
            },
            "response": {
                "key": "project_group_id",
                "name": "<%- name %>"
            }
        },
        "identity.Provider": {
            "request": {
                "search": ["provider", "name"]
            },
            "response": {
                "key": "provider",
                "name": "<%- name %>"
            }
        },
        "identity.ServiceAccount": {
            "request": {
                "search": ["service_account_id", "name"]
            },
            "response": {
                "key": "service_account_id",
                "name": "<%- name %>"
            }
        },
        "identity.User": {
            "request": {
                "search": ["user_id", "name"]
            },
            "response": {
                "key": "user_id",
                "name": "<%if (name) { %><%- name %> <% } %><<%- user_id %>>"
            }
        },
        "secret.Secret": {
            "request": {
                "search": ["secret_id", "name"]
            },
            "response": {
                "key": "secret_id",
                "name": "<%- name %>"
            }
        },
        "inventory.CloudServiceType": {
            "request": {
                "only": ["cloud_service_type_id", "name", "group", "provider", "tags"],
                "search": ["cloud_service_type_id", "name", "group"]
            },
            "response": {
                "key": "cloud_service_type_id",
                "name": "<%- group %> > <%- name %>",
                "data": {
                    "id": "<%- cloud_service_type_id %>",
                    "name": "<%- name %>",
                    "group": "<%- group %>",
                    "provider": "<%- provider %>",
                    "icon": "<%- tags['spaceone:icon'] %>"
                }
            }
        },
        "inventory.Collector": {
            "request": {
                "search": ["collector_id", "name"]
            },
            "response": {
                "key": "collector_id",
                "name": "<%- name %>"
            }
        },
        "inventory.Region": {
            "request": {
                "search": ["name", "provider", "region_code"]
            },
            "response": {
                "key": "region_code",
                "name": "[<%- provider %>] <%- name %> | <%- region_code %>"
            }
        },
        "monitoring.Webhook": {
            "request": {
                "search": ["webhook_id", "name"]
            },
            "response": {
                "key": "webhook_id",
                "name": "<%if (name) { %><%- name %> <% } %><<%- webhook_id %>>"
            }
        },
        "cost_analysis.DataSource": {
            "request": {
                "search": ["data_source_id", "name"]
            },
            "response": {
                "key": "data_source_id",
                "name": "<%- name %>"
            }
        },
        "inventory.CloudServiceQuerySet": {
            "request": {
                "search": ["query_set_id", "name"]
            },
            "response": {
                "key": "query_set_id",
                "name": "<%- name %>"
            }
        }
    }
}
