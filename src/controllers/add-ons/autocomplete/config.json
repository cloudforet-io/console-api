{
    "resourceTypes": {
        "identity.Project": {
            "request": {
                "search": ["project_id", "name"],
                "only": ["project_id", "name", "project_group_info"]
            },
            "response": {
                "key": "project_id",
                "name": "<%- project_group_info.name %> > <%- name %>"
            }
        },
        "identity.ProjectGroup": {
            "request": {
                "search": ["project_group_id", "name"]
            },
            "response": {
                "key": "project_group_id",
                "name": "<%- name %>"
            }
        },
        "identity.Provider": {
            "request": {
                "search": ["provider", "name"]
            },
            "response": {
                "key": "provider",
                "name": "<%- name %>"
            }
        },
        "identity.ServiceAccount": {
            "request": {
                "search": ["service_account_id", "name"]
            },
            "response": {
                "key": "service_account_id",
                "name": "<%- name %>"
            }
        },
        "identity.User": {
            "request": {
                "search": ["user_id", "name"]
            },
            "response": {
                "key": "user_id",
                "name": "<%if (name) { %><%- name %> <% } %><<%- user_id %>>"
            }
        },
        "secret.Secret": {
            "request": {
                "search": ["secret_id", "name"]
            },
            "response": {
                "key": "secret_id",
                "name": "<%- name %>"
            }
        },
        "inventory.CloudServiceType": {
            "request": {
                "search": ["cloud_service_type_id", "name", "group"]
            },
            "response": {
                "key": "secret_service_type_id",
                "name": "<%- group %> > <%- name %>"
            }
        },
        "inventory.Collector": {
            "request": {
                "search": ["collector_id", "name"]
            },
            "response": {
                "key": "collector_id",
                "name": "<%- name %>"
            }
        },
        "inventory.Region": {
            "request": {
                "search": ["name", "provider", "region_code"]
            },
            "response": {
                "key": "region_code",
                "name": "[<%- provider %>] <%- name %> | <%- region_code %>"
            }
        },
        "monitoring.Webhook": {
            "request": {
                "search": ["webhook_id", "name"]
            },
            "response": {
                "key": "webhook_id",
                "name": "<%if (name) { %><%- name %> <% } %><<%- webhook_id %>>"
            }
        }
    }
}
